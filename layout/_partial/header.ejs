<div class="alignleft">
  <h1><a href="<%- config.root %>"><%= config.title %></a></h1>
  <h2><a href="<%- config.root %>"><% if (config.subtitle){ %><%= config.subtitle %><% } %></a></h2>
</div>

<div class="alignright">
  <div class="social alignright">
    <% if (theme.facebook_id){ %>
      <a class="facebook" href="http://www.facebook.com/<%= theme.facebook_id %>" title="Facebook">Facebook</a>
    <% } %>
    <% if (theme.google_plus_id){ %>
      <a class="google" href="https://plus.google.com/<%= theme.google_plus_id %>" title="Google+">Google+</a>
    <% } %>
    <% if (theme.twitter_id){ %>
      <a class="twitter" href="http://twitter.com/<%= theme.twitter_id %>" title="Twitter">Twitter</a>
    <% } %>
    <% if (theme.github_id){ %>
      <a class="github" href="https://github.com/<%= theme.github_id %>" title="GitHub">GitHub</a>
    <% } %>
    <a class="rss" href="<% if (theme.rss){ %><%- theme.rss %><% } else { %><%- config.root %>atom.xml<% } %>" title="RSS">RSS</a>
  </div>

  <div>
    <nav id="main-nav" class="alignright">
      <ul>
        <% for (var i in theme.menu){ %>
          <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
        <% } %>
      </ul>
      <div class="clearfix"></div>
    </nav>
  </div>

</div>
<div class="clearfix"></div>
